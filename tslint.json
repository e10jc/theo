{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "**/node_modules/**"
    ]
  },
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "interface-name": false,
    "no-console": [true, "warn"],
    "quotemark": false,
    "semicolon": false,
    "space-before-function-paren": false,
    "trailing-comma": false
  },
  "rulesDirectory": []
}
